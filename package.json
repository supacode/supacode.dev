{
  "name": "supacode.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "live": "next start",
    "lint": "next lint",
    "lint:scss": "stylelint \"**/*.scss\"",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run lint:scss && npm run check-lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "next": "^13.0.0",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-prism": "^1.3.6",
    "sass": "^1.54.9",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.5",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/remark-prism": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.0",
    "postcss-preset-env": "^7.8.2",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.3"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 1
      }
    }
  }
}
